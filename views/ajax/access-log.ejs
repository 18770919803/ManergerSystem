<section id="widget-grid" class="">
    <div class="row">
        <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="alert alert-info">
                <form class="form-inline">
                    <div class="form-group controls input-append date form_date margin-right margin-top form-group1">
                        <label for='startTime'>时间范围: </label>
                        <input type="text" class="form-control form-controler1" name="startTime" id="startTime"
                               size="16" readonly
                               data-date="" data-date-format="yyyy-mm-dd" data-link-field="dtp_input2"
                               data-link-format="yyyy-mm-dd"/>
                    </div>
                    <div for="endTime"
                         class="form-group controls input-append date form_date margin-top margin-right form-group1">
                        <label for='endTime'>-</label>
                        <input type="text" class="form-control form-controler1" name="endTime" id="endTime" size="16"
                               readonly
                               data-date="" data-date-format="yyyy-mm-dd" data-link-field="dtp_input2"
                               data-link-format="yyyy-mm-dd"/>
                    </div>
                    <div class="form-group margin-right form-group1">
                        <label>用户名:</label> <input type="text" class="form-control form-controler1" name="username"
                                                   id="username"/>
                    </div>
                    <div class="form-group margin-right form-group1">
                        <label>IP:</label> <input type="text" class="form-control form-controler1" name="ip" id="ip"/>
                    </div>
                    <div class="form-group margin-right form-group1">
                        <label>URL:</label> <input type="text" class="form-control form-controler1" name="url"
                                                   id="url"/>
                    </div>
                    <div class="form-group margin-right ">
                        <a href="javascript:void(0)" id="searchBtn" class="btn btn-primary" style="margin-top:7px;"><i
                                    class="fa fa-search"></i>搜索</a>
                    </div>
                </form>
            </div>
            <table id="ID_gridTable"></table>
            <div id="ID_gridPager"></div>
            <br>
        </article>
    </div>
</section>

