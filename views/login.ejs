<head>
    <meta charset="utf-8"/>
    <title> 日志审计和非法关键字过滤管理系统</title>
    <meta name="description" content="日志审计和非法关键字过滤管理系统"/>
    <meta name="author" content="日志审计和非法关键字过滤管理系统"/>
    <meta http-equiv="X-UA-Compatible" content="IE=Edge"/>
    <meta http-equiv="content-type" content="text/html;charset=utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no"/>
    <link rel="stylesheet" type="text/css" media="screen" href="/css/bootstrap.min.css"/>
    <link rel="stylesheet" type="text/css" media="screen" href="/css/font-awesome.min.css"/>
    <link rel="stylesheet" type="text/css" media="screen" href="/css/smartadmin-production-plugins.min.css"/>
    <link rel="stylesheet" type="text/css" media="screen" href="/css/smartadmin-production.min.css"/>
    <link rel="stylesheet" type="text/css" media="screen" href="/css/demo.min.css"/>
    <style>
        html {
            width: 100%;
            height: 100%;
            margin: 0;
            padding: 0;
        }

        body {
            margin: 0;
            padding: 0;
        }

        #main {
            padding: 0;
            padding-bottom: 52px;
            height: 400px;
            position: absolute;
            top: 0;
            left: 0;
            bottom: 0;
            right: 0;
            width: 100%;
            background: none;
            margin: 10% auto;
            overflow: hidden;
        }

        label {
            margin-bottom: 6px !important;
        }

        em.invalid {
            margin-left: 10px;
        }
    </style>
    <script type="text/javascript" src="/js/supportIe.js"></script>
    <script type="text/javascript" src="/js/referrer.js"></script>
</head>
<body class="animated fadeInDown">
<div id="main" role="main">
    <div id="content" class="container">
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-5 col-lg-4" style=' margin: 0 auto;'>
                <div class="well no-padding login-surface">
                    <form id="login-form" class="smart-form client-form">
                        <header>
                            <div class="login-head">日志管理系统</div>
                        </header>
                        <fieldset>
                            <section class="checking">
                                <label class="label">用户名</label>
                                <label class="input"> <i class="icon-append fa fa-user"></i>
                                    <input type="text" name="username" id='username'/>
                                    <b class="tooltip tooltip-top-right"><i class="fa fa-user txt-color-teal"></i>
                                        请输入您的用户名</b></label>
                            </section>
                            <section class="checking">
                                <label class="label">密码</label>
                                <label class="input"> <i class="icon-append fa fa-lock"></i>
                                    <input type="password" name="password" id='password'/>
                                    <b class="tooltip tooltip-top-right"><i class="fa fa-lock txt-color-teal"></i>
                                        请输入您的密码</b> </label>
                            </section>

                            <section>
                                <label class="checkbox">
                                    <input type="checkbox" name="remerber" id="remerber" class='login-rember'
                                           checked="true"/>
                                    <i></i>记住密码</label>

                            </section>
                        </fieldset>
                        <footer>
                            <div>
                                <button type="button" class="btn btn-primary login-btn" id='submit'>
                                    登录
                                </button>
                            </div>
                        </footer>
                    </form>

                </div>

            </div>
        </div>
    </div>

</div>
<div class="page-footer"
     style='text-align: center;background: none;height: 52px;line-height: 52px;padding:0 !important;'>
